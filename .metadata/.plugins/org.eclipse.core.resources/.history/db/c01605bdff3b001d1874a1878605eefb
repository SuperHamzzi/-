package StudentRunner;

public class Rectangle {
	private int length;
	private int width;

	public Rectangle(int length, int width) {
		this.length = length;
		this.width = width;
	
	}

	public int area() {
		return length*width;
	}
	
	public String toString() {
		return String.format("width:%d,length:%d,area:%d",width,length,area);
	}

}
