package java_primitve_data_types;

public class BiNumber {
	private int number1;
	private int number2;
	
	public BiNumber(int number1, int number2) {
		this.number1 = number1;
		this.number2 = number1;
		
	}
	public int multiyply(){
		return number1*number2;
	}
	

	public int add() {
		return number1 + number2;
	}

}
